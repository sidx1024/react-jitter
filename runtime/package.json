{
  "name": "react-jitter-runtime",
  "private": true,
  "version": "1.0.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "fast-equals": "^5.2.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": ">=16.8.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external react --external react-dom",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react --external react-dom",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public"
  }
}
